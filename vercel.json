{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "PORT": "5000",
      "MONGODB_URI": "@mongodb_uri",
      "JWT_SECRET": "@jwt_secret"
    }
  }
  